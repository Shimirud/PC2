@model List<Usuario>

<h1>Datos del Usuario</h1>

@if (Model.Count == 0) {
    <div class="alert alert-warning text-center">ERROR. Int√©ntelo de nuevo.</div>
} else {
    <ol>
    @foreach (var u in Model) {
        <li class="d-flex">
            <div>@u.Nombres</div>
            <div>@u.Apellidos</div>
            <div>@u.Email</div>
            <div>@u.Password</div>
        </li>
    }
    </ol>
}